{
  "title": "CloudMind Backend Overview",
  "timezone": "browser",
  "panels": [
    {
      "type": "stat",
      "title": "Request Rate",
      "gridPos": {"x": 0, "y": 0, "w": 6, "h": 4},
      "targets": [{"expr": "sum(rate(http_requests_total[5m]))"}]
    },
    {
      "type": "graph",
      "title": "HTTP 5xx Rate",
      "gridPos": {"x": 6, "y": 0, "w": 6, "h": 6},
      "targets": [{"expr": "sum(rate(http_requests_total{status=~\"5..\"}[5m]))"}]
    },
    {
      "type": "graph",
      "title": "p95 Latency",
      "gridPos": {"x": 0, "y": 6, "w": 6, "h": 6},
      "targets": [{"expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket[5m])) by (le))"}]
    },
    {
      "type": "table",
      "title": "Top Endpoints by Errors",
      "gridPos": {"x": 6, "y": 6, "w": 6, "h": 8},
      "targets": [{"expr": "topk(10, sum(rate(http_requests_total{status=~\"5..\"}[5m])) by (endpoint))"}]
    }
  ],
  "schemaVersion": 36,
  "version": 1
}


