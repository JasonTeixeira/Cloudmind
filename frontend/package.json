    "test:visual": "playwright test",
    "test:accessibility": "jest --testNamePattern='accessibility|a11y'",
    "test:performance": "node scripts/performance-test.mjs",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e && npm run test:visual",
